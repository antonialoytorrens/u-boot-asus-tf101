/dts-v1/;

#include "tegra30.dtsi"

/ {
	model = "ASUS Transformer";
	compatible = "asus,transformer", "nvidia,tegra30";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	aliases {
		mmc0 = &sdmmc4;	/* eMMC */
		mmc1 = &sdmmc1; /* uSD slot */

		usb0 = &dock_usb;

		serial0 = &uarta;
	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	uarta: serial@70006000 {
		status = "okay";
	};

	i2c@7000d000 {
		status = "okay";
		clock-frequency = <400000>;
	};

	sdmmc1: mmc@78000000 {
		status = "okay";
		cd-gpios = <&gpio TEGRA_GPIO(I, 5) GPIO_ACTIVE_LOW>;
		bus-width = <4>;
	};

	sdmmc4: sdhci@78000600 {
		status = "okay";
		bus-width = <8>;
		non-removable;
	};

	/* Dock's USB port */
	dock_usb: usb@7d008000 {
		status = "okay";
	};

	clocks {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		clk32k_in: clock@0 {
			compatible = "fixed-clock";
			reg=<0>;
			#clock-cells = <0>;
			clock-frequency = <32768>;
		};
	};
};
